# Farcaster Mini App Configuration
# Copy this to .env and fill in your values

# Your deployment domain (e.g., moonlander-abc123.vercel.app)
VITE_APP_DOMAIN=moonlander-game.example.com

# Farcaster signer public key (from manifest signing)
VITE_FARCASTER_SIGNER_KEY=0x...

# Your webhook URL for notifications (optional)
VITE_WEBHOOK_URL=https://moonlander-game.example.com/api/webhook

# Optional: Farcaster user FID for testing
VITE_TEST_FID=
